<?xml version="1.0" encoding="utf-8"?>
<template xmlns="http://napkinlaf.sf.net/sketch/"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <title>Test Template</title>
    <description>Describes a template used for testing</description>
    <clippingBounds>
        <dimensions>
            <width>80</width>
            <height>80</height>
        </dimensions>
    </clippingBounds>
    <templateItem>
        <drawStroke>true</drawStroke>
        <drawFill>false</drawFill>
        <strokeWeight>2</strokeWeight>
        <strokeColor>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </strokeColor>
        <fillColor>
            <r>255</r>
            <g>0</g>
            <b>0</b>
        </fillColor>
        <straightLine>
            <start>
                <x>0</x>
                <y>0</y>
            </start>
            <end>
                <x>50</x>
                <y>50</y>
            </end>
        </straightLine>
    </templateItem>
    <templateItem>
        <drawStroke>true</drawStroke>
        <drawFill>false</drawFill>
        <strokeWeight>2</strokeWeight>
        <strokeColor>
            <r>0</r>
            <g>0</g>
            <b>255</b>
        </strokeColor>
        <quadLine>
            <start>
                <x>50</x>
                <y>0</y>
            </start>
            <control>
                <x>5</x>
                <y>5</y>
            </control>
            <end>
                <x>0</x>
                <y>50</y>
            </end>
        </quadLine>
    </templateItem>
    <templateItem>
        <drawStroke>true</drawStroke>
        <drawFill>false</drawFill>
        <strokeWeight>1</strokeWeight>
        <strokeColor>
            <r>0</r>
            <g>255</g>
            <b>0</b>
        </strokeColor>
        <cubicLine>
            <start>
                <x>0</x>
                <y>0</y>
            </start>
            <control>
                <x>50</x>
                <y>10</y>
            </control>
            <control>
                <x>30</x>
                <y>30</y>
            </control>
            <end>
                <x>50</x>
                <y>0</y>
            </end>
        </cubicLine>
    </templateItem>
    <templateItem>
        <drawStroke>true</drawStroke>
        <drawFill>false</drawFill>
        <strokeWeight>.3</strokeWeight>
        <strokeColor>
            <r>0</r>
            <g>0</g>
            <b>0</b>
        </strokeColor>
        <path>
            <moveTo>
                <point>
                    <x>10</x>
                    <y>0</y>
                </point>
            </moveTo>
            <lineTo>
                <point>
                    <x>20</x>
                    <y>60</y>
                </point>
            </lineTo>
            <quadTo>
                <control>
                    <x>50</x>
                    <y>30</y>
                </control>
                <point>
                    <x>70</x>
                    <y>60</y>
                </point>
            </quadTo>
            <cubicTo>
                <control>
                    <x>100</x>
                    <y>50</y>
                </control>
                <control>
                    <x>50</x>
                    <y>30</y>
                </control>
                <point>
                    <x>30</x>
                    <y>0</y>
                </point>
            </cubicTo>
            <close/>
        </path>
    </templateItem>
</template>
